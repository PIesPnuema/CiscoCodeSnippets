    // Protocol configurations
    // -----------------------
    // This section is for cisco protocols. Feel free to add notes that are helpful to know when adding configurations
    //

    "AAA Configuration": {
        "prefix": "cpAAA",
        "scope": "cisco",
        "body": [
            "! Configure AAA (Authentication, Authorization, and Accounting)",
            "! Modify the placeholders accordingly.",
            "",
            "! Enable new-model for AAA configurations",
            "aaa new-model",
            "",
            "! Configure AAA group for RADIUS server",
            "aaa group server radius <RADIUS-GROUP>",
            "  server name <RADIUS-SERVER-NAME>",
            "",
            "! Configure authentication methods",
            "aaa authentication login VTY <VTY-AUTH-METHOD>",
            "aaa authentication login CONSOLE <CONSOLE-AUTH-METHOD>",
            "aaa authentication enable default <ENABLE-AUTH-METHOD>",
            "aaa authentication dot1x default group <DOT1X-AUTH-GROUP>",
            "",
            "! Configure authorization methods",
            "aaa authorization exec default <EXEC-AUTH-METHOD>",
            "aaa authorization exec VTY <VTY-EXEC-AUTH-METHOD>",
            "aaa authorization network default group <NETWORK-AUTH-GROUP>",
            "aaa authorization auth-proxy default group <AUTH-PROXY-AUTH-GROUP>",
            "",
            "! Configure periodic accounting update",
            "aaa accounting update periodic <UPDATE-PERIOD>",
            "aaa accounting auth-proxy default start-stop group <AUTH-PROXY-ACCT-GROUP>",
            "aaa accounting dot1x default start-stop group <DOT1X-ACCT-GROUP>",
            "",
            "! Configure dynamic authorization server",
            "aaa server radius dynamic-author",
            "  client <RADIUS-SERVER-IP> server-key <RADIUS-SERVER-KEY>",
            "",
            "! Configure session ID handling and device sensors",
            "aaa session-id common",
            "device-sensor accounting",
            "device-sensor notify all-changes"
        ],
        "description": "AAA Configuration"
    },
    "Configure SNMP": {
        "prefix": "cpSNMP",
        "scope": "cisco",
        "body": [
            "! Run ?snmp command for more information about configuring SNMP",
            "snmp-server community <COMMUNITY-STRING> RO <ACCESS-LIST-NUMBER>",
            "snmp-server trap-source <SOURCE-INTERFACE>",
            "snmp-server source-interface informs <INFORMS-INTERFACE>",
            "snmp-server enable traps snmp linkdown linkup",
            "snmp-server enable traps mac-notification change move threshold",
            "snmp-server host <HOST-IP-ADDRESS> version 2c <COMMUNITY-STRING> [mac-notification]",
            "access-list <ACCESS-LIST-NUMBER> permit <ALLOWED-SOURCE-IP>",
            "access-list <ACCESS-LIST-NUMBER> deny any",
            "snmp-server community <COMMUNITY-STRING> use-acl <ACCESS-LIST-NUMBER>",
            "snmp-server enable"
        ],
        "description": "Configure SNMP settings in Cisco devices"
    },
    "Configure RADIUS Server": {
        "prefix": "cpRadiusServ",
        "scope": "cisco",
        "body": [
            "radius-server vsa send accounting                     // Enable sending VSAs in accounting",
            "radius-server vsa send authentication                 // Enable sending VSAs in authentication",
            "radius-server attribute 1 format ietf upper-case      // Configure User-Name format and uppercase",
            "radius-server attribute 4 include-in-access-req       // Include NAS-IP-Address attribute in Access-Request",
            "radius-server attribute 5 include-in-access-req       // Include NAS-Port attribute in Access-Request",
            "radius-server attribute 6 on-for-login-auth           // Include Service-Type attribute for login authentication",
            "radius-server attribute 6 support-multiple            // Support multiple instances of Service-Type attribute",
            "radius-server attribute 7 include-in-access-req       // Include Framed-Protocol attribute in Access-Request",
            "radius-server attribute 8 include-in-access-req       // Include Framed-IP-Address attribute in Access-Request",
            "radius-server attribute 11 include-in-access-req      // Include Filter-Id attribute in Access-Request",
            "radius-server attribute 12 include-in-access-req      // Include Framed-IP-Address attribute in Access-Request",
            "radius-server attribute 24 include-in-access-req      // Include State attribute in Access-Request",
            "radius-server attribute 25 access-request include     // Include Class attribute in Access-Request",
            "radius-server attribute 26 include-in-access-req      // Include Vendor-Specific attribute in Access-Request",
            "radius-server attribute 31 mac format ietf upper-case // Configure MAC format and uppercase in Calling-Station-ID",
            "radius-server attribute 31 send nas-port-detail       // Send additional NAS port detail in Calling-Station-ID",
            "radius-server attribute 32 include-in-access-req      // Include Framed-IPv6-Prefix attribute in Access-Request",
            "radius-server attribute 44 include-in-access-req      // Include NAS-IPv6-Address attribute in Access-Request",
            "radius-server attribute 61 include-in-access-req      // Include NAS-Port-Type attribute in Access-Request",
            "radius-server attribute 62 include-in-access-req      // Include Port-Limit attribute in Access-Request",
            "radius-server attribute 64 include-in-access-req      // Include Tunnel-Type attribute in Access-Request",
            "radius-server attribute 65 include-in-access-req      // Include Tunnel-Medium-Type attribute in Access-Request",
            "radius-server attribute 66 include-in-access-req      // Include Tunnel-Private-Group-ID attribute in Access-Request",
            "radius-server attribute 81 include-in-access-req      // Include Framed-Management-Protocol attribute in Access-Request",
            "radius-server attribute 82 include-in-access-req      // Include Framed-Management-Policy-Id attribute in Access-Request"
        ],
        "description": "Configure enhanced attributes for RADIUS server"
    },

    "IP SLA icmp-echo configurations": {
        "prefix": "cpIpSlaIcmpEcho",
        "scope": "cisco",
        "body": [
            "! icmp-echo option. There are ",
            "! \"track\" tracking number and \"ip sla\" numbers are arbitrary",
            "ip sla <ip sla ID#>",
            "   icmp-echo <ip address to ping> source-ip <IP>",
            "   frequency <# in Seconds (10 is normal)>",
            "exit",
            "",
            "! start service",
            "ip sla schedule <ip sla ID#> life forever start-time now",
            "",
            "! Creating a tracking object of Track ID#",
            "track <Track ID#> ip sla <ip sla ID#> reachability",
            "",
            "!Attaches \"track object\" to route which tells the router to only use this route if the tracking object is up",
            "ip route <IP> <SUBNET> <NEXT HOP> track <Track ID#>",
        ],
        "description": "IP SLA icmp-echo configurations"
    },
    "Rapid Per VLAN Spanning Tree +": {
        "prefix": "cpRpvst+",
        "scope": "cisco",
        "body": [
            "spanning-tree mode rapid-pvst",
            " ",
            "! vlan syntax: Lower Priority number is the prefered route.", 
            "! Default to Lowest MAC address for Root Bridge",
            "vlan <VLAN ID>",
            "name <CampusData>",
            "spanning-tree vlan <VLAN ID> priority 0",
        ],
        "description": "Rapid Per VLAN Spanning Tree + creation"
    },
    "HSRP standby commands": {
        "prefix": "cpStandby",
        "scope": "cisco",
        "body": [
            "standby <ID> ip <IP>",
            "standby <ID> priority 110",
            "standby <ID> timers <Seconds for next Hello> <Hold time before switching to backup>",
            "!standby <ID> preempt"
        ],
        "description": "HSRP standby commands"
    },     
    "OSPF Area Configuration": {
        "prefix": "cpOspf",
        "scope": "cisco",
        "body": [
            "router ospf <PROCESS ID>",
            "   router-id <IP>",
            "   redistribute connected subnets",
            "   redistribute static subnets",
            "   redistribute <PROTOCOL> metric <METRIC>",
            "   !default-information originate",
            "   log-adjacency-changes",
            "   network <IP> <WILD CARD MASK> area 0",
            "   network <IP> <WILD CARD MASK> area 1",
            "   area <AREA ID> authentication message-digest",
            "   !passive-interface <GigabitEthernet0/1>",
            "   !passive-interface default",
            "   !no passive-interface vlan <30>",
            "   !no passive-interface <po14>",
            "   !no passive-interface <G2/1>",
            "exit",
        ],
        "description": "OSPF Area level Configuration"
    },
    "OSPF Interface Configuration": {
        "prefix": "cpOspfInterface",
        "scope": "cisco",
        "body": [
            "! Illustrates what can be done with the interface configuration",
            "interface <INTERFACE>",
            "   ip ospf <PROCESS_ID> area <AREA>",
            "   ip ospf priority <PRIORITY>",
            "   ip ospf cost <COST>",
            "   ip ospf hello-interval <HELLO_INTERVAL>",
            "   ip ospf dead-interval <DEAD_INTERVAL>",
            "   ip ospf retransmit-interval <RETRANSMIT_INTERVAL>",
            "   ip ospf network <NETWORK_TYPE>",
            "   ip ospf authentication message-digest",
            "   ip ospf message-digest-key <KEY_ID> md5 <SECRET>",
            "interface <INTERFACE>",
            "   ip ospf <PROCESS_ID> area <AREA>",
            "   ip ospf priority <PRIORITY>",
            "   ip ospf cost <COST>",
            "   ip ospf hello-interval <HELLO_INTERVAL>",
            "   ip ospf dead-interval <DEAD_INTERVAL>",
            "   ip ospf retransmit-interval <RETRANSMIT_INTERVAL>",
            "   ip ospf network <NETWORK_TYPE>",
            "   ip ospf authentication null",
            "exit",
        ],
        "description": "OSPF Interface Configuration, showing what can be done at the interface"
    },
    "EIGRP Configuration": {
        "prefix": "cpEigrp",
        "scope": "cisco",
        "body": [
            "router eigrp <AS_NUMBER>",
            "   eigrp router-id <ROUTER_ID>",
            "   network <NETWORK> <WILDCARD_MASK>",
            "   passive-interface default",
            "   no passive-interface <INTERFACE>",
            "   distribute-list <ACCESS_LIST> in",
            "   distribute-list <ACCESS_LIST> out",
            "   maximum-paths <MAX_PATHS>",
            "   variance <VARIANCE>",
            "   traffic-share balanced",
            "   delay minimum <DELAY>",
            "   bandwidth-percent <BANDWIDTH_PERCENT>",
            "   auto-summary",
            "exit",
            "interface <INTERFACE>",
            "   ip summary-address eigrp <AS_NUMBER> <SUMMARY_ADDRESS> <SUBNET_MASK>",
            "   ip bandwidth-percent eigrp <AS_NUMBER> <PERCENTAGE>",
            "   ip hello-interval eigrp <AS_NUMBER> <SECONDS>",
            "   ip hold-time eigrp <AS_NUMBER> <SECONDS>",
            "exit",
        ],
        "description": "EIGRP Configuration"
    },
    "BGP Configuration": {
        "prefix": "cpBgp",
        "scope": "cisco",
        "body": [
            "! Type command \"cpPrefixRouteMap\"",
            "router bgp <LOCAL_ASN>",
            "   bgp log-neighbor-changes",
            "   bgp router-id <ROUTER_ID>",
            "   network <NETWORK> mask <SUBNET_MASK>",
            "   neighbor <NEIGHBOR_IP> remote-as <REMOTE_ASN>",
            "   neighbor <NEIGHBOR_IP> description <DESCRIPTION>",
            "   neighbor <NEIGHBOR_IP> update-source <INTERFACE>",
            "   neighbor <NEIGHBOR_IP> ebgp-multihop 2",
            "   neighbor <NEIGHBOR_IP> password <PASSWORD>",
            "   neighbor <NEIGHBOR_IP> timers <KEEPALIVE> <HOLDTIME>",
            "   neighbor <NEIGHBOR_IP> route-map <ROUTE_MAP_NAME> in",
            "   neighbor <NEIGHBOR_IP> route-map <ROUTE_MAP_NAME> out",
            "   maximum-paths <NUMBER_OF_PATHS>",
            "exit",
        ],
        "description": "BGP Configuration"
    },
    "Prefix List and Route Map Configuration": {
        "prefix": "cpPrefixRouteMap",
        "scope": "cisco",
        "body": [
            "ip prefix-list <PREFIX_LIST_IN_NAME> seq 10 permit <NETWORK>",
            "ip prefix-list <PREFIX_LIST_OUT_NAME> seq 20 deny <NETWORK>",

            "route-map <ROUTE_MAP_IN_NAME> permit 10",
            "   match ip address prefix-list <PREFIX_LIST_IN_NAME>",
            "!route-map <ROUTE_MAP_IN_NAME> deny 20",
            
            "route-map <ROUTE_MAP_OUT_NAME> permit 10",
            "   match ip address prefix-list <PREFIX_LIST_OUT_NAME>",
            "!route-map <ROUTE_MAP_OUT_NAME> deny 20"
        ],
        "description": "Prefix List and Route Map Configuration"
    },
    "IPSec Tunnel Configuration": {
        "prefix": "cpIPSecTunnel",
        "scope": "cisco",
        "body": [
            "crypto isakmp policy 10",
            "   encryption aes 256",
            "   authentication pre-share",
            "   group 14",
            "crypto isakmp key <KEY> address <PEER_IP_ADDRESS>",
            "crypto ipsec transform-set MYSET esp-aes 256 esp-sha-hmac",
            "   mode tunnel",
            "crypto map <MYMAP> 10 ipsec-isakmp",
            "   set peer <PEER_IP_ADDRESS>",
            "   set transform-set MYSET",
            "   match address <ACCESS_CONTROL_LIST>",
            "interface <INTERFACE>",
            "   crypto map <MYMAP>",
            "exit",
        ],
        "description": "IPSec Tunnel Configuration"
    },
    "NAT Overload Configuration": {
        "prefix": "cpNatOverload",
        "scope": "cisco",
        "body": [
            "! Configure Port Address Translation (PAT) needs Dynamic NAT configured",
            "",
            "ip nat pool NAT_POOL 203.0.113.1 203.0.113.10 netmask 255.255.255.0",
            "access-list 1 permit 192.168.1.0 0.0.0.255",
            "",
            "interface GigabitEthernet0/0",
            "  ip nat inside",
            "",
            "interface GigabitEthernet0/1",
            "  ip nat outside",
            "",
            "ip nat inside source list 1 pool NAT_POOL overload"
        ],
        "description": "NAT Overload Configuration"
    },
    "Dynamic NAT Configuration": {
        "prefix": "cpDynNat",
        "scope": "cisco",
        "body": [
            "interface <INTERFACE_OUTSIDE>",
            "   ip address <OUTSIDE_IP> <SUBNET_MASK>",
            "   ip nat outside",
            "exit",
            "interface <INTERFACE_INSIDE>",
            "   ip address <INSIDE_IP> <SUBNET_MASK>",
            "   ip nat inside",
            "exit",
            "ip nat pool <NAT_POOL_NAME> <START_IP> <END_IP> netmask <NETMASK>",
            "ip access-list standard <ACL_NAME>",
            "   permit <INSIDE_NETWORK> <WILDCARD_MASK>",
            "exit",
            "ip nat inside source list <ACL_NAME> pool <NAT_POOL_NAME>"
        ],
        "description": "Dynamic NAT Configuration"
    },
    "ACCESS_LIST": {
        "prefix": "cACL",
        "scope": "cisco",
        "body": [
            "! ?ACL command shows examples on writing ACLs ",
            "access-list <acl-number> {permit|deny} <protocol> <source> <source-wildcard> <destination> <destination-wildcard> [eq <port>]",
        ],
        "description": "Access list syntax"
    },
 
