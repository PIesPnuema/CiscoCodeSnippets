    "How to: AAA Configurations": {
        "prefix": "?aaa",
        "scope": "cisco",
        "body": [
            "! Configure AAA (Authentication, Authorization, and Accounting)",
            "! This example demonstrates AAA configurations in Cisco devices.",
            "",
            "! Enable informational logging for monitoring",
            "logging monitor informational",
            "",
            "! Enable new-model for AAA configurations",
            "aaa new-model",
            "",
            "! Configure AAA group for RADIUS server",
            "aaa group server radius ISE-GROUP",
            "  server name ISE",
            "",
            "! Configure authentication methods",
            "aaa authentication login VTY local",
            "aaa authentication login CONSOLE local",
            "aaa authentication enable default enable",
            "aaa authentication dot1x default group ISE-GROUP",
            "",
            "! Configure authorization methods",
            "aaa authorization exec default local",
            "aaa authorization exec VTY local",
            "aaa authorization network default group ISE-GROUP",
            "aaa authorization auth-proxy default group ISE-GROUP",
            "",
            "! Configure periodic accounting update",
            "aaa accounting update periodic 5",
            "aaa accounting auth-proxy default start-stop group ISE-GROUP",
            "aaa accounting dot1x default start-stop group ISE-GROUP",
            "",
            "! Configure dynamic authorization server",
            "aaa server radius dynamic-author",
            "  client <ISE_SERVER_IP_ADDRESS> server-key cisco123",
            "  server-key cisco123",
            "",
            "! Configure session ID handling and device sensors",
            "aaa session-id common",
            "device-sensor accounting",
            "device-sensor notify all-changes"
        ],
        "description": "How to configure AAA (Authentication, Authorization, and Accounting) settings in Cisco devices"
    },
