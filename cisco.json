    "Cisco Initial Configuration": {
        "prefix": "cInitial",
        "scope": "cisco",
        "body": [
            "! transport input ssh is to only allow SSH",
            "enable",
            "configure terminal",
            "hostname <DEVICE NAME>",
            "enable secret <DEVICE PASSWORD>",
            "username <DEVICE USERNAME> secret <USERNAME PASSWORD>",
            "line con 0",
            "logging synchronous",
            "login local",
            "line vty 0 4",
            "transport input ssh",
            "logging synchronous",
            "login local"
        ],
        "description": "Cisco Initial Configuration"
    },
    "VLAN Creation": {
        "prefix": "vlan",
        "scope": "cisco",
        "body": [
            "vlan ",
            "name ",
